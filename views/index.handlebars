                    <div class="col-12 card p-3 m-3">
                        <h3>{{name}}</h3><br>
                        <p>{{description}}</p><br>
                        <ul>
                            {{#if website}}
                            <li>
                                Website: <a href="{{website}}">{{website}}</a>
                            </li>
                            {{/if}}
                            {{#if url}}
